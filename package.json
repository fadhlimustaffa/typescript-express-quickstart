    "repository": {
        "type": "git",
        "url": "https://github.com/fadhlimustaffa/typescript-express-quickstart.git"
    },
    "dependencies": {
        "@types/config": "^0.0.33",
        "@types/express": "^4.0.39",
        "@types/node": "^6.0.46",
        "config": "^1.28.1",
        "express": "^4.16.2",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "web-request": "^1.0.7"
    },
    "devDependencies": {}
}
